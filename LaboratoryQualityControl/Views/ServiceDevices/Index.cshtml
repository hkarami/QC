                <a asp-action="Details" asp-route-id="@item.ServiceDeviceID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ServiceDeviceID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
