@model LaboratoryQualityControl.Domain.DeviceMaintenance

@{
    ViewData["Title"] = "نگهداری تجهیزات جدید";
}

<h2>@ViewData["Title"]</h2>

@*<h4>DeviceMaintenance</h4>*@
<hr />
<div class="row">
    <div class="col-md-12">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            @Html.Partial("_PartialCreateEdit",Model)
            <hr />
            <div class="form-group">
                <input type="submit" value="ثبت" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">برگشت به لیست</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
